{
  "name": "flightworks",
  "version": "0.6.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --scheme flightworks-dev",
    "ios:release": "react-native run-ios --configuration Release",
    "start": "react-native start",
    "start:web": "webpack serve --mode development --hot --env target=dev",
    "build:web": "webpack --mode production --env target=dev",
    "build:web:staging": "npm run build:web -- --env target=staging",
    "build:web:prod": "npm run build:web -- --env target=prod",
    "deploy": "cd modules/firebase && firebase use dev && firebase deploy",
    "deploy:staging": "cd modules/firebase && firebase use staging && firebase deploy",
    "deploy:prod": "cd modules/firebase && firebase use prod && firebase deploy",
    "deploy:web": "npm run deploy -- --only hosting",
    "deploy:web:staging": "npm run deploy:staging -- --only hosting",
    "deploy:web:prod": "npm run deploy:prod -- --only hosting",
    "deploy:functions": "npm run deploy -- --only functions",
    "deploy:functions:staging": "npm run deploy:staging -- --only functions",
    "deploy:functions:prod": "npm run deploy:prod -- --only functions",
    "fetch-firestore-indexes": "cd modules/firebase && firebase use dev && firebase firestore:indexes > firestore.indexes.json",
    "test": "jest",
    "lint": "eslint .",
    "generate-icons": "npx @svgr/cli -d modules/components/theme/icons --ignore-existing assets/logo",
    "demo:web": "webpack serve --mode production --hot --env target=prod",
    "demo:mobile": "npm run ios:release -- --scheme flightworks-prod --simulator='iPhone 13' && npm run ios:release -- --scheme flightworks-prod --simulator='iPhone 13 Pro'"
  },
